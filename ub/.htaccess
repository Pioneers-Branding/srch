<IfModule mod_rewrite.c>
# enable rewrite engine
RewriteEngine On
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
Header always set Content-Security-Policy "upgrade-insecure-requests;"

# if requested url does not exist pass it as path info to index.php
RewriteRule ^$ index.php?/ [QSA,L]
RewriteRule ^login/?$ login.php [L,QSA]
RewriteRule ^register/?$ register.php [L,QSA]
RewriteRule ^checkout/?$ checkout.php [L,QSA]
RewriteRule ^checkout_m/?$ checkout_m.php [L,QSA]
RewriteRule ^order/?$ order.php [L,QSA]
RewriteRule ^orders/?$ orders.php [L,QSA]
RewriteRule ^order_new/?$ order_new.php [L,QSA]
RewriteRule ^contact/?$ contact.php [L,QSA]
RewriteRule ^forgetPassword/?$ forgetPassword.php [L,QSA]
RewriteRule ^contact/?$ contact.php [L,QSA]
RewriteRule ^thank-you/?$ thank-you.php [L,QSA]
RewriteRule ^search/?$ search.php [L,QSA]
RewriteRule ^product-filter/?$ product-filter.php [L,QSA]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule (.*) all-page.php?/$1 [QSA,L]
</IfModule>

